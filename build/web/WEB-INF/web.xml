<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>controladorArtista</servlet-name>
        <servlet-class>co.com.soundMusic.Artista.controladorArtista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorUsuario</servlet-name>
        <servlet-class>co.com.soundMusic.Login.Usuario.controladorUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorEmpresaDifusora</servlet-name>
        <servlet-class>co.com.soundMusic.EmpresaDifusora.controladorEmpresaDifusora</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorLogin</servlet-name>
        <servlet-class>co.com.soundMusic.Login.controladorLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorLogAuditoria</servlet-name>
        <servlet-class>co.com.soundMusic.LogAuditoria.controladorLogAuditoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controladorArtistaEmpresa</servlet-name>
        <servlet-class>co.com.soundMusic.Negocio.Regalias.ArtistaEmpresa.controladorArtistaEmpresa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorRegalias</servlet-name>
        <servlet-class>co.com.soundMusic.Negocio.Regalias.ControladorRegalias</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controladorArtista</servlet-name>
        <url-pattern>/controladorArtista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorUsuario</servlet-name>
        <url-pattern>/controladorUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorEmpresaDifusora</servlet-name>
        <url-pattern>/controladorEmpresaDifusora</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorLogin</servlet-name>
        <url-pattern>/controladorLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorLogAuditoria</servlet-name>
        <url-pattern>/controladorLogAuditoria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controladorArtistaEmpresa</servlet-name>
        <url-pattern>/controladorArtistaEmpresa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorRegalias</servlet-name>
        <url-pattern>/ControladorRegalias</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
